#carol
crencas: { estaEnsolarado ; tenhoProtetorSolar ; }
objetivos: { passarProtetorSolar ; naoPegarSol ; }
planos: { 
    plano1 ( passarProtetorSolar ; estaEnsolarado E tenhoProtetorSolar ;
        { pegarProtetorSolar; passarProtetorSolar; } ) ;
    plano2 ( naoPegarSol ; NAO estaEnsolarado ; { ficarEmCasa; assistirFilme ; } ) ;
    plano3 ( naoPegarSol ; estaEnsolarado E tenhoProtetorSolar ;
        { ficarEmCasa ; lerLivro ; } ) ;
}%